@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
<div class="wrapper wrp-contact">
        <div class="container">
            <div class="row">
                <div class="col-12 heading-text">
                    <h1>CONTACT &amp; LOCATION INFORMATION</h1>
                    <p>The first step is never easy and so rarely do you know where to go or who to talk to. Drop us a line and someone will contact you as soon as possible regarding your factoring needs.</p>
                </div>
                <div class="col-12 google-maps">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1768.4605253104146!2d-99.50120914414073!3d27.564960582851107!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x866120f7e677ba5f%3A0x9191a84ad3427107!2sFaro%20Investment%20Managment!5e0!3m2!1sen!2sin!4v1601543388129!5m2!1sen!2sin" width="100%" height="620" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    
                    <div class="row location-outer">
                        <div class="col-12">
                            <h2>Laredo, Texas</h2>
                        </div>
                        <div class="col-12 col-sm-12 col-md-6 col-lg-6 nopadd">
                            <a href="tel:" title="">
                                <img src="https://gibsonads.com/gibsondevelopment/faro-factoring/wp-content/themes/faro/images/contact-phone.png" alt="" title="">
                                <span>@Model.Value("mobile")</span>
                            </a>
                            <a href="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1768.4605253104146!2d-99.50120914414073!3d27.564960582851107!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x866120f7e677ba5f%3A0x9191a84ad3427107!2sFaro%20Investment%20Managment!5e0!3m2!1sen!2sin!4v1601543388129!5m2!1sen!2sin" title="CONTACT &amp; LOCATION INFORMATION">
                            <img src="https://gibsonads.com/gibsondevelopment/faro-factoring/wp-content/themes/faro/images/contact-address.png" alt="" title="">
                                <span>@Model.Value("address")</span>
                            </a>
                        </div>
                        <div class="col-12 col-sm-12 col-md-6 col-lg-6 nopadd">
                            <a href="javascript:void(0)" title="Office Hours">
                            <img src="https://gibsonads.com/gibsondevelopment/faro-factoring/wp-content/themes/faro/images/time.png" alt="" title="">
                                
                                <span>
                                Office Hours:<br>
                                Monday - Thursday 8:30 AM - 5:30 PM<br>Friday 8:30 AM - 4:00 PM<br>                                </span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>